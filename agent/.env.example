# Base wallet private key (hex, 0x...)
# Generate one with: npm run generate-wallet
WALLET_PRIVATE_KEY=

# Base Builder Code from base.dev
BUILDER_CODE=

# Agent config
CYCLE_INTERVAL_MS=900000
# Custom RPC URL for Base (optional — avoids rate limits on default public RPC)
# RPC_URL=https://base-mainnet.infura.io/v3/YOUR_KEY

# Heartbeat model (cheap, fast — survival checks + swaps)
LLM_HEARTBEAT_MODEL=openai/gpt-oss-120b
# Strategy model (smarter — analysis + trading decisions)
LLM_STRATEGY_MODEL=anthropic/claude-sonnet-4
# Below this → survival triggers (emergency)
USDC_SURVIVAL_THRESHOLD=3
# Strategy keeps this much idle (buffer for inference costs between cycles)
USDC_IDLE_TARGET=5
